{
  "auto_detect": "yes",
  "customize": {
    "add_include_path": "yes",
    "static_lib": "libblis.lib",
    "env_prefix": "CK_ENV_LIB_BLIS",
    "file_root_name": "libblis",
    "include_file": "blis.h",
    "limit_recursion_dir_search": {
      "linux": 4,
      "win": 4
    },
    "skip_file_check": "yes",
    "soft_file": {
      "win": "libblis.lib"
    }
  },
  "soft_name": "Blis library (prebuilt (compiled with clang))",
  "tags": [
    "lib",
    "blis"
  ]
}
